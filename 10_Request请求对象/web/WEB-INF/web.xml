<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>RequestServlet</servlet-name>
        <servlet-class>com.sun.controller.RequestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>twoServlet</servlet-name>
        <servlet-class>com.sun.controller.twoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>threeServlet</servlet-name>
        <servlet-class>com.sun.controller.threeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RequestServlet</servlet-name>
        <url-pattern>/test/one.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>twoServlet</servlet-name>
        <url-pattern>/two.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>threeServlet</servlet-name>
        <url-pattern>/three.do</url-pattern>
    </servlet-mapping>
</web-app>